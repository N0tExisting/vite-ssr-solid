{
	"name": "solid-ssr",
	"version": "1.0.0",
	"description": "",
	"author": "Alexandre Mouton-Brady",
	"license": "ISC",
	"keywords": [
		"solid",
		"solid-js",
		"ssr"
	],
	"type": "commonjs",
	"main": "index.js",
	"scripts": {
		"dev": "node ./server",
		"build": "concurrently --kill-others-on-fail -n build: \"pnpm:build:*\"",
		"build:client": "vite build --outDir dist/client",
		"build:server": "vite build --ssr src/server/index.ts --outDir dist/server",
		"start": "cross-env NODE_ENV=production node ./dist/server/",
		"optimize": "vite optimize --force"
	},
	"dependencies": {
		"compression": "^1.7.4",
		"cross-fetch": "^3.1.5",
		"express": "^4.17.2",
		"serve-static": "^1.14.2",
		"solid-app-router": "^0.2.1",
		"solid-js": "^1.3.5",
		"solid-meta": "^0.27.3",
		"tslib": "^2.3.1"
	},
	"devDependencies": {
		"@faker-js/faker": "^6.0.0-alpha.5",
		"@types/compression": "^1.7.2",
		"@types/express": "^4.17.13",
		"@types/serve-static": "^1.13.10",
		"concurrently": "^7.0.0",
		"cross-env": "^7.0.3",
		"jiti": "^1.12.15",
		"sass": "^1.49.7",
		"typescript": "^4.5.5",
		"vite": "^2.7.13",
		"vite-plugin-pages-solid": "^0.0.2",
		"vite-plugin-solid": "^2.2.5",
		"vite-tsconfig-paths": "^3.3.17"
	}
}
